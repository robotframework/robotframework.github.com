{
    "comments": {
      "lineComment": "#"
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"]
    ],
    "autoClosingPairs": [
      {
        "open": "{",
        "close": "}"
      },
      {
        "open": "[",
        "close": "]"
      },
      {
        "open": "(",
        "close": ")"
      },
      {
        "open": "\"",
        "close": "\"",
        "notIn": ["string"]
      }
    ],
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["\"", "\""],
      ["'", "'"]
    ],
    "folding": {
      "offSide": true,
      "markers": {
        "start": "^\\s*#\\s*region\\b",
        "end": "^\\s*#\\s*endregion\\b"
      }
    }
  }
  